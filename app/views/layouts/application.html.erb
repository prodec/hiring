<!DOCTYPE html>
<html>
	<head>
	  <title>Prodec</title>
	  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
	  <%= javascript_include_tag '//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js', :defer => true, :async => true %>
	  <%= csrf_meta_tags %>
	</head>
	<body>

		<div class="container">
			<% flash.each do |key, value| %>
		        <div class="<%= flash_class(key) %>">
		              <%= value %>
		        </div>
		  	<% end %>
		  	<div class="container">
		  		<%= yield %>
		  	</div>
		</div>

	</body>
</html>
